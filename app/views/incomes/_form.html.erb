<%= simple_form_for @income  do |f| %>
  <%= f.input :description %>
  <%= f.input :paid_at, as: :string, input_html: { class: :date } %>
  <%= f.input :expires_at, as: :string, input_html: { class: :date } %>
  <%= f.input :category %>
  <%= f.input :value %>
  <%= f.input :paid %>

  <%= f.submit class: 'btn btn-success' %>
<% end %>
